{
  "name": "progen-apollo-gql-server",
  "version": "1.0.0",
  "description": "Middlelayer for the Progen application built with graphql and apollo.",
  "main": "server.ts",
  "private": true,
  "scripts": {
    "start": "env-cmd ts-node-dev --debounce=1000 ./src/server.ts",
    "types": "ts-node ./scripts/generate-types-from-schema.ts && prettier --no-config --write \"src/types/TypesGraphQL.ts\" --ignore-path .gitignore"
  },
  "keywords": [
    "Apollo",
    "GraphQl"
  ],
  "author": "Daniel Persson",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.22.2",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node-fetch": "^2.5.7",
    "graphql-schema-typescript": "^1.3.2",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
